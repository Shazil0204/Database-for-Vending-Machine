@page
@model AdminSide.Pages.showAllModel
@{
}

<link href="https://cdn.jsdelivr.net/npm/bootstrap@5.0.2/dist/css/bootstrap.min.css" rel="stylesheet"
	  integrity="sha384-EVSTQN3/azprG1Anm3QDgpJLIm9Nao0Yz1ztcQTwFspd3yD65VohhpuuCOmLASjC" crossorigin="anonymous">

<link href="/css/checkbox.css" rel="stylesheet">
<link href="/css/showAll.css" rel="stylesheet" />
<div class="card-container">


	<div class="d-flex">

		<a asp-page="/Admin">

			<button type="submit" style="margin-top: 1vh; height: 5vh;">Back</button>

		</a>

		<div style="margin: 5px;"></div>

		<button type="submit" style="margin-top: 1vh; height: 5vh;">Next</button>

	</div>

	@foreach (string item in Model.VMs())
	{
		<button class="card-button">

			<div class="card">
				<img src="/Images/1.png" alt="Image Description">
				<div class="card-text">
					<h1>@item</h1>
					<h3>Is Vending Machine Operational</h3>
					<label class="switch">
						<input type="checkbox">
						<span class="slider round"></span>
					</label>
				</div>
			</div>
		</button>
	}

</div>